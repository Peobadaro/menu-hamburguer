{
    "version": 2,
    "builds": [
        {
            "src": "*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/imagem/(.*)",
            "dest": "/imagem/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
} 